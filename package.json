{
  "name": "cloudflare-pages",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 3000",
    "start": "node dist/index.js",
    "build": "vite build",
    "preview": "vite preview --host 0.0.0.0 --port 3000",
    "deploy": "pnpm run build && wrangler pages deploy",
    "types-generate": "pnpm openapi-typescript https://maverick-public-services-stage.dev.gotickets.com/api/open-api -o ./src/services/maverick/generated/maverick-schema.d.ts",
    "check-env": "node scripts/get-env.js",
    "load-env": "node scripts/load-env.js",
    "check-toml": "node scripts/toml-to-env.js",
    "format": "pnpm prettier 'src/**/*.{ts,tsx}' --write",
    "format-check": "pnpm prettier 'src/**/*.{ts,tsx}' --check",
    "lint": "pnpm eslint --cache",
    "lint-fix": "pnpm eslint --cache --fix",
    "validate": "pnpm tsc --noEmit",
    "prepare": "husky",
    "test": "vitest",
    "test-coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@fontsource-variable/plus-jakarta-sans": "^5.1.1",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-portal": "^1.1.9",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@sentry/react": "^8.54.0",
    "@tanstack/react-query": "^5.63.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "js-cookie": "^3.0.5",
    "lodash-es": "^4.17.21",
    "openapi-fetch": "^0.13.4",
    "react": "^19.0.0",
    "react-day-picker": "^8.10.1",
    "react-dom": "^19.0.0",
    "react-hotkeys-hook": "^4.6.1",
    "react-indiana-drag-scroll": "^2.2.1",
    "react-router": "^7.1.1",
    "react-use": "^17.6.0",
    "ua-parser-js": "^2.0.3",
    "use-local-storage-state": "^19.5.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241218.0",
    "@eslint/js": "^9.18.0",
    "@tailwindcss/postcss": "^4.1.3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/js-cookie": "^3.0.6",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@types/react-window": "^1.8.8",
    "@types/universal-cookie": "^3.0.0",
    "@vitejs/plugin-legacy": "^6.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "3.0.2",
    "cross-fetch": "^4.1.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-interactive": "^11.1.0",
    "eslint-plugin-perfectionist": "^4.6.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^15.14.0",
    "happy-dom": "^16.6.0",
    "husky": "^9.1.7",
    "lightningcss": "^1.29.1",
    "lint-staged": "^15.4.1",
    "msw": "^2.7.0",
    "openapi-typescript": "^7.5.2",
    "postcss": "^8.4.49",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "smol-toml": "^1.3.1",
    "tailwindcss": "^4.1.3",
    "terser": "^5.37.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.0.11",
    "vitest": "^3.0.2",
    "wrangler": "^3.96.0"
  }
}
